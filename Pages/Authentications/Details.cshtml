@page
@model WebApplication1.Pages.Authentications.DetailsModel

<div>
    <h4>Authentication</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Куратор
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Authentication.IdCurator)
        </dd>
        <dt class="col-sm-2">
            Логин
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Authentication.Login)
        </dd>
        <dt class="col-sm-2">
            Пароль
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Authentication.Password)
        </dd>
        <dt class="col-sm-2">
            Тип пользователя
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Authentication.IdUser)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.Authentication?.IdAuthentication">Изменить</a> |
    <a asp-page="./Index">Вернуться к таблице</a>
</div>
