@page
@model WebApplication1.Pages.EngagementWithParentss.DeleteModel

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>EngagementWithParents</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Дата
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.EngagementWithParents.DateConversation)
        </dd>
        <dt class="col-sm-2">
            Студент
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.EngagementWithParents.IdStudent)
        </dd>
        <dt class="col-sm-2">
            Суть
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.EngagementWithParents.EssenceOfConversation)
        </dd>
        <dt class="col-sm-2">
            Принятое решение
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.EngagementWithParents.AcceptedDecision)
        </dd>
    </dl>
    
    <form method="post">
        <input type="hidden" asp-for="EngagementWithParents.IdEngagementWithParents" />
        <input type="submit" value="Удалить" class="btn btn-danger" /> |
        <a asp-page="./Index">Вернуться к таблице</a>
    </form>
</div>
