        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ActiveGroup.ScholarshipCommission)
        </dd>
    </dl>
    
    <form method="post">
        <input type="hidden" asp-for="ActiveGroup.IdActiveGroup" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-page="./Index">Вернуться к таблице</a>
