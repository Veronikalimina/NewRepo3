@page
@model WebApplication1.Pages.ExtracurricularEmploymentOfStudentss.IndexModel

<p>
    <a asp-page="Create">Создать запись</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Курс
            </th>
            <th>
                Студент
            </th>
            <th>
                Деятельность в колледже
            </th>
            <th>
                Деятельность вне колледжа
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.ExtracurricularEmploymentOfStudents) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdCourse)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdStudent)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CollegeActivity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ActivitiesOutsideCollege)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.IdExtracurricularEmploymentOfStudents">Изменить</a> |
                <a asp-page="./Details" asp-route-id="@item.IdExtracurricularEmploymentOfStudents">Просмотр</a> |
                <a asp-page="./Delete" asp-route-id="@item.IdExtracurricularEmploymentOfStudents">Удалить</a>
            </td>
        </tr>
}
    </tbody>
</table>
